{"ast":null,"code":"import Aside from \"./components/Aside.vue\";\nimport Cookie from \"js-cookie\";\nexport default {\n  name: \"App\",\n\n  data() {\n    return {\n      key: \"c4ca4238a0b923820dcc509a6f75849b\",\n      timestamp: \"\",\n      sign: \"\",\n      userList: []\n    };\n  },\n\n  watch: {\n    $route(to, from) {\n      window.location.reload();\n    }\n\n  },\n  computed: {\n    loginName() {\n      return Cookie.get(\"username\");\n    },\n\n    loginSwitch() {\n      return Cookie.get(\"loginSwitch\");\n    }\n\n  },\n\n  mounted() {\n    // 时间戳\n    let time = parseInt(new Date().getTime() / 1000);\n    this.timestamp = time;\n    this.$axios.post(\"/api/Apibase/getSign\", {\n      key: this.key,\n      timestamp: this.timestamp,\n      page: 1,\n      limit: 10\n    }).then(res => {\n      this.sign = res.data.data;\n      this.$axios.post(\"/api/user/userList\", {\n        page: 1,\n        limit: 10,\n        sign: this.sign,\n        timestamp: time,\n        key: this.key\n      }).then(res => {\n        this.userList = res.data.data.list;\n      });\n    });\n  },\n\n  methods: {\n    leave() {\n      Cookie.remove(\"username\");\n      Cookie.remove(\"loginSwitch\");\n      Cookie.remove(\"loginId\");\n      location.reload();\n    }\n\n  },\n  components: {\n    Aside\n  }\n};","map":{"version":3,"mappings":"AAyDA;AACA;AAEA;EACAA,WADA;;EAEAC;IACA;MACAC,uCADA;MAEAC,aAFA;MAGAC,QAHA;MAIAC;IAJA;EAMA,CATA;;EAUAC;IACAC;MACAC;IACA;;EAHA,CAVA;EAeAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA;;EANA,CAfA;;EAuBAC;IACA;IACA;IACA;IACA,YACAC,IADA,CACA,sBADA,EACA;MACAX,aADA;MAEAC,yBAFA;MAGAW,OAHA;MAIAC;IAJA,CADA,EAOAC,IAPA,CAOAC;MACA;MACA,YACAJ,IADA,CACA,oBADA,EACA;QACAC,OADA;QAEAC,SAFA;QAGAX,eAHA;QAIAD,eAJA;QAKAD;MALA,CADA,EAQAc,IARA,CAQAC;QACA;MACA,CAVA;IAWA,CApBA;EAqBA,CAhDA;;EAiDAC;IACAC;MACAC;MACAA;MACAA;MACAC;IACA;;EANA,CAjDA;EAyDAC;IACAC;EADA;AAzDA","names":["name","data","key","timestamp","sign","userList","watch","$route","window","computed","loginNam